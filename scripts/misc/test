#! /usr/bin/env python


from uweclang import *
# import re
from pprint import pprint


pat = re.compile('^' + DATE_REGEX_ANY.pattern + '$', re.IGNORECASE | re.VERBOSE)

if pat.match('12 / 12 / 1221'):
    print(pat.match('12 / 12 / 1221').groups())

if pat.match('12 Jan 1202'):
    print(pat.match('12 Jan 1202').groups())

if pat.match('12 Dec 2015'):
    print(pat.match('12 Dec 2015').groups())

if pat.match('12 December 2015 and a date'):
    print(pat.match('12 December 2015').groups())

if pat.match('Not a date'):
    print(pat.match('Not a date').groups())
